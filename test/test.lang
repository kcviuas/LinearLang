letUn copyList(x:LinList) = 
    if-null x
    then null
    else split x as carx cdrx
         copy carx as xx1 xx2
         letLin yy = copyAll(cdrx)
         split yy as yy1 yy2
         letLin rt1 = liCons(xx1,yy1)
         letLin rt2 = liCons(xx2,yy2)
         liCons(rt1,rt2)


letUn freeList(x:LinList) = 
    if-null x
    then free(x)
    else split x as x1 x2
         begin 
            free(x);
            freeList(x2)
         end


